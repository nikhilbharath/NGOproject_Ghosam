<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>GhoSam</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>LoginServlet</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.mvc.controller.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>Logout</display-name>
    <servlet-name>Logout</servlet-name>
    <servlet-class>com.mvc.controller.Logout</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>FormController</display-name>
    <servlet-name>FormController</servlet-name>
    <servlet-class>com.mvc.controller.FormController</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>EditFormController</display-name>
    <servlet-name>EditFormController</servlet-name>
    <servlet-class>com.mvc.controller.EditFormController</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>DeleteFormController</display-name>
    <servlet-name>DeleteFormController</servlet-name>
    <servlet-class>com.mvc.controller.DeleteFormController</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>DeleteEventController</display-name>
    <servlet-name>DeleteEventController</servlet-name>
    <servlet-class>com.mvc.controller.DeleteEventController</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>PassObjectForm</display-name>
    <servlet-name>PassObjectForm</servlet-name>
    <servlet-class>com.mvc.controller.PassObjectForm</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>EventForm</display-name>
    <servlet-name>EventForm</servlet-name>
    <servlet-class>com.mvc.controller.EventForm</servlet-class>
  </servlet>
   <servlet>
    <description></description>
    <display-name>EODController</display-name>
    <servlet-name>EODController</servlet-name>
    <servlet-class>com.mvc.controller.EODController</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>ExportList</display-name>
    <servlet-name>ExportList</servlet-name>
    <servlet-class>com.mvc.controller.ExportList</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>EODController</servlet-name>
    <url-pattern>/Views/EODController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FormController</servlet-name>
    <url-pattern>/Views/FormController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/Logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EditFormController</servlet-name>
    <url-pattern>/Views/EditFormController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteFormController</servlet-name>
    <url-pattern>/Views/DeleteFormController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteEventController</servlet-name>
    <url-pattern>/Views/DeleteEventController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PassObjectForm</servlet-name>
    <url-pattern>/Views/PassObjectForm</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ExportList</servlet-name>
    <url-pattern>/Views/ExportList</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EventForm</servlet-name>
    <url-pattern>/Views/EventForm</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>SessionFilter</filter-name>
    <filter-class>com.mvc.controller.SessionFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>SessionFilter</filter-name>
    <url-pattern>/Views/*</url-pattern>
  </filter-mapping>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
</web-app>